#Name of the customer 
export CC_CUSTOMER=demo

#Customer environment eg dev, stage, prod
export CC_CUSTOMER_ENV=dev

#Your cloud region eg: westus2  
export CC_REGION=westus2

#Domain name to be used for the customer deployment
export CC_DOMAIN_NAME=iacce.xyz




#Your Account name eg:john.deo
export CC_ACCOUNT_USER='myname'

#Your Azure login eg: john.deo@abc.com
export CC_AZURE_USER='myname@domain.com'

#Your Azure subscribtions
export CC_SUBSCRIPTION='azure-subscription-code'



#Your container registry url is auto generated, replace CC_CONTAINER_REGISTRY with your ACR name if you want to use shared ACR for all environments.
#To access your ACR in other region run replication to the next region 
#./az/acr-replica.sh "eastus" will replicate your acr in the next region, also for DR
#export CC_CONTAINER_REGISTRY="acrname"
export CC_CONTAINER_REGISTRY_URL=${CC_CONTAINER_REGISTRY}.azurecr.io

#Your git repo url
export CC_GIT_URL=http://git.infospica.com


